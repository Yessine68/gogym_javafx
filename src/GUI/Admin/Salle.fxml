            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../Images/Icons/previous.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <TextField fx:id="entrySearch" layoutX="789.0" layoutY="22.0" />
