<AnchorPane id="AnchorPane" prefHeight="586.0" prefWidth="1200.0" style="-fx-background-color: #bbdefb;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Admin.SalleController">
