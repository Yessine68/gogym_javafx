      <Button fx:id="btnSupp" layoutX="1070.0" layoutY="472.0" mnemonicParsing="false" onAction="#DeleteSalle" prefHeight="57.0" prefWidth="120.0" style="-fx-background-color: red;" text="Supprimer" textAlignment="CENTER" textFill="WHITE">
