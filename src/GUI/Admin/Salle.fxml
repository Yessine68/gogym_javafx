      </Button>
      <Button fx:id="btnModif" graphicTextGap="5.0" layoutX="1070.0" layoutY="383.0" mnemonicParsing="false" onAction="#UpdateSalle" prefHeight="57.0" prefWidth="120.0" style="-fx-background-color: orange;" text="Modifier" textAlignment="CENTER" textFill="WHITE">
